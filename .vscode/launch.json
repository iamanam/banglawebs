{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}",
            "stopOnEntry": false,
            "args": ["--debug-port","5000"],
            "cwd": "meteor",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--debug-port"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": false,
            "outDir": null
        },
        {
            "name": "Attach",
             "preLaunchTask": "meteor",
            "type": "node",
            "request": "attach",
            "port": 5002
        }
    ]
}